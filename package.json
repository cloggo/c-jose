{
    "name": "c-jose",
    "description": "jose - node c binding",
    "version": "1.0.3",
    "main": "index.js",
    "scripts": {
        "test": "mocha -R spec"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.delite.ca/docker/lib/node/c-jose.git"
    },
    "engines": {
        "node": ">=8.4.0"
    },
    "keywords": [
        "jws",
        "jwe",
        "jwk",
        "jwa",
        "jwt",
        "jose",
        "crypto",
        "cryptography",
        "rsa",
        "dsa",
        "ecdsa",
        "ecies",
        "ecdh"
    ],
    "author": "Aaron Diep <aaron@delite.ca>",
    "contributors": [
        "Nathaniel McCallum <npmccallum@redhat.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "node-pre-gyp": "^0.6.32"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "supertest": "^3.1.0",
        "nodemon": "^1.18.0"
    },
    "gypfile": true,
    "binary": {
        "module_name": "native_rt",
        "module_path": "./lib/binding/{configuration}/{platform}-{arch}/",
        "remote_path": "./{module_name}/v{version}/{configuration}/",
        "package_name": "{module_name}-v{version}-{platform}-{arch}.tar.gz",
        "host": "https://nodeaddons.s3-us-west-2.amazonaws.com"
    },
    "scripts": {
        "preinstall": "npm install node-pre-gyp",
        "install": "node-pre-gyp install --fallback-to-build"
    }
}
